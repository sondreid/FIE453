library(randomForest)
library(caret)
library(doParallel)
cl <- makePSOCKcluster(5)
registerDoParallel(cl)
y <- df$retx
x <- df %>% select(-retx)
control <- trainControl(method = "cv",
number = 5,
verboseIter = TRUE)
set.seed(1)
mtry <- sqrt(ncol(x))
tunegrid <- expand.grid(.mtry = mtry)
start_time <- Sys.time()
rf <- train(x, y,
method = "rf",
importance = TRUE,
tuneGrid = tunegrid,
trControl = control)
end_time <- Sys.time()
stopCluster(cl)
stopCluster(cl)
# Most important features
varImp(rf)
